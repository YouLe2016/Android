<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.wyl.android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name=".lifecycles.LifecycleActivity"
            android:label="Lifecycle">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".viewmodel.SaveStateViewModelActivity"
            android:label="@string/save_state_vieModel" />
        <activity
            android:name=".viewmodel.AndroidViewModelActivity"
            android:label="@string/android_viewModel" />
        <activity
            android:name=".screen.OrientationActivity"
            android:label="Screen Orientation" />
        <activity
            android:name=".databinding.DataBindingActivity"
            android:label="DataBinding" />
        <activity
            android:name=".livedata.LiveDataActivity"
            android:label="LiveData" />
        <activity
            android:name=".viewmodel.ViewModelActivity"
            android:label="ViewModel" />
        <activity
            android:name=".navigation.MainActivity"
            android:label="Navigation"></activity>
    </application>

</manifest>